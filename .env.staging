# Application
APP_NAME="SpikeUp Organization (Staging)"
APP_ENV=staging
APP_KEY=
APP_DEBUG=true
APP_URL=https://staging.spikeup-org.netlify.app

# Logging
LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug
LOG_SLACK_WEBHOOK_URL=

# Database
DB_CONNECTION=mysql
DB_HOST=staging-db.example.com
DB_PORT=3306
DB_DATABASE=spikeup_staging
DB_USERNAME=staging_user
DB_PASSWORD=

# Broadcasting, Cache, Session, Queue
BROADCAST_DRIVER=redis
CACHE_DRIVER=redis
FILESYSTEM_DISK=local
QUEUE_CONNECTION=redis
SESSION_DRIVER=redis
SESSION_LIFETIME=120
SESSION_SECURE_COOKIE=true
SESSION_DOMAIN=.staging.spikeup-org.netlify.app

# Redis
REDIS_HOST=staging-redis.example.com
REDIS_PASSWORD=null
REDIS_PORT=6379
REDIS_PREFIX=spikeup_staging_

# Mail
MAIL_MAILER=smtp
MAIL_HOST=smtp.mailtrap.io
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS=staging@spikeup.com
MAIL_FROM_NAME="${APP_NAME}"

# Sanctum
SANCTUM_STATEFUL_DOMAINS=staging.spikeup-org.netlify.app
SESSION_DOMAIN=.staging.spikeup-org.netlify.app
SANCTUM_EXPIRATION=null

# CORS
CORS_ALLOWED_ORIGINS=https://staging.spikeup-org.netlify.app

# Integrations
SLACK_WEBHOOK_URL=
MS_TEAMS_WEBHOOK_URL=

# Performance & Security
CACHE_PREFIX=spikeup_staging_
QUEUE_TIMEOUT=60
QUEUE_RETRY_AFTER=90
QUEUE_FAILED_DRIVER=database-uuids

# Storage
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=
AWS_BUCKET=staging-spikeup-org
AWS_URL=
AWS_USE_PATH_STYLE_ENDPOINT=false

# Rate Limiting
RATE_LIMIT=60
RATE_LIMIT_TIME=1

# Optimization
VITE_APP_ENV=staging
VITE_APP_URL="${APP_URL}"
ASSET_URL="${APP_URL}"

# Monitoring
TELESCOPE_ENABLED=true
HORIZON_ENABLED=true